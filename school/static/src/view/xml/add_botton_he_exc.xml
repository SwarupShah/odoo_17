<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="school.modelInfoBtn" t-inherit="web.FormView">
        <xpath expr="//t[@t-set-slot='layout-actions']" position="inside">
            <button id="mybtn" class="btn btn-primary">Info
            </button>
        </xpath>
    </t>
    <t t-name="school.modelHrBtn" t-inherit="hr_expense.ListButtons" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button type="button" class="btn btn-secondary" t-on-click="actionhrList">Download Report</button>
        </xpath>
    </t>
