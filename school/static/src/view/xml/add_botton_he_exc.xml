    <t t-name="school.modelSchoolForm" t-inherit="web.FormStatusIndicator" t-inherit-mode="extension">
        <xpath expr="//div[@class='o_form_status_indicator_buttons d-flex']" position="inside">
            <button type="button" class="o_form_button_cancel btn btn-light px-1 py-0 lh-sm" t-on-click="() => this.actionFormIcon()"><i class="fa fa-solid fa-user"></i></button>
        </xpath>
