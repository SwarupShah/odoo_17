            <button type="button" class="btn btn-secondary" t-on-click="() => this.actionhrList()">Send Mail</button>
        </xpath>
    </t> -->
    <t t-name="school.modelSaleBtn" t-inherit="web.ListView">
        <xpath expr="//t[@t-set-slot='control-panel-always-buttons']" position="inside">
            <button type="button" class="btn btn-secondary" t-on-click="() => this.actionSaleList()">Download Report</button>
        </xpath>
    </t>
